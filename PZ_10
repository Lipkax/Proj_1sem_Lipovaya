market = {"Магистр": ["Лермонтов", "Достоевский", "Пушкин", "Тютчев"],
          "ДомКниг": ["Толстой", "Грибоедов", "Чехов", "Пушкин"],
          "Букмаркет": ["Пушкин", "Достоевский", "Маяковский"],
          "Галерея": ["Чехов", "Тютчев", "Пушкин"]}

dost = ', '.join([i for i in market.keys() if 'Достовевский' and 'Пушкин' in market[i]])
print(dost)
